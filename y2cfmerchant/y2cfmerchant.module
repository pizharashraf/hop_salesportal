function y2cfmerchant_menu_alter(&$items) {
	global $user;
	if($user->uid != '1') {
		$items['node/%node/edit']['type'] = MENU_CALLBACK;
		$items['user/%user/edit']['type'] = MENU_CALLBACK;
		unset($items['user/%user/edit']['type']);
		
	}
	return $items;
}
