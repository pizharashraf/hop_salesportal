function homepage_i3feature_block($op = 'list', $delta = 0, $edit = array()) {
  $blocks = array();
  if ($op == 'list') {
    $blocks[0] = array(
      'info' => t('i3sc_homepage_slider'), 
      );
    $blocks[1] = array(
      'info' => t('i3sc_homepage_i3feature'), 
      );
    $blocks[2] = array(
      'info' => t('i3sc_homepage_headermiddle'), 
      );
    $blocks[3] = array(
      'info' => t('i3sc_homepage_topheader'), 
      );
    $blocks[4] = array(
      'info' => t('i3sc_footers'), 
      );

    return $blocks;
  }
  elseif ($op == 'view') {
    switch ($delta) {
      case 0:
        $block = array(
          'subject' => t(''), 
          'content' => i3sc_homepage_slider(),
        );
      break;
      case 1:
        $block = array(
          'subject' => t(''), 
          'content' => i3sc_homepage_i3feature(),
        );
      break;

    case 2:
        $block = array(
          'subject' => t(''), 
          'content' => i3sc_homepage_headermiddle(),
        );
      break;
    case 3:
        $block = array(
          'subject' => t(''), 
          'content' => i3sc_homepage_topheader(),
        );
      break;

    case 4:
        $block = array(
          'subject' => t(''), 
          'content' => i3sc_footers(),
        );
      break;
	  
     
    }
    return $block;
  }
}


