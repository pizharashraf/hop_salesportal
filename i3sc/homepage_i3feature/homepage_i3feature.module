function i3sc_homepage_i3feature(){
  global $base_url;
  $sql_i3features = db_query("select nn.nid, nn.title, nr.body,ff.filepath from {content_type_home_page_i3_feature} cthps inner join {node} nn on cthps.nid=nn.nid inner join {files} ff on cthps.field_feature_image_fid=ff.fid inner join {node_revisions} nr on nn.nid=nr.nid");


  $output = '';
  while($rs = db_fetch_object($sql_i3features)) { 

    $output .= '<div class="home-bottombox">';
    $output .= '<h2>'.$rs->title.'</h2>';
    $output .= '<div class="home-bottomimgbox"><img src="'.$base_url.'/'.$rs->filepath.'" height="145" width="290" /></div>';				
    $output .= '<p>'.$rs->body.'</p>';				
    $output .= '</div>';
  } 
  return $output;
}
